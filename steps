sudo apt update
sudo apt install build-essential git git-lfs cmake
git clone https://github.com/NVIDIA/TensorRT-LLM.git
cd TensorRT-LLM
git submodule update --init --recursive
git lfs install
git lfs pull
sudo ln -s /usr/include /usr/lib/include
# Install python3.12
python3.12 scripts/build_wheel.py --cpp --clean
